


##############################################原始表变更
2.文件表新增字段：“文件分类”
ALTER TABLE sys_file_info ADD file_type  varchar(64)  NULL DEFAULT NULL COMMENT '文件分类' AFTER file_bucket;
3.字典表新增：“字典类型编码”（同时将字典类型数据同步到字典表）
ALTER TABLE sys_dict ADD dict_type_code  varchar(64)  NULL DEFAULT NULL COMMENT '字典类型编码' AFTER dict_type_id;
UPDATE SYS_DICT T1 SET DICT_TYPE_CODE=(SELECT T2.CODE FROM SYS_DICT_TYPE T2 WHERE T1.DICT_TYPE_ID=T2.DICT_TYPE_ID);



#############################################个人表ddl
#1.客户表
#DROP TABLE `TB_MATCHMAKING_CUSTOMER`;
CREATE TABLE `TB_MATCHMAKING_CUSTOMER` (
 `CUSTOMER_ID` VARCHAR(64)  NOT NULL PRIMARY KEY COMMENT '主键ID',
 `NAME` VARCHAR(200)  NOT NULL  COMMENT '姓名',
 `GENDER` VARCHAR(2)  NOT NULL COMMENT '性别（字典：SEX）',
 `HOUSEHOLD_PROVINCE` VARCHAR(16)  DEFAULT NULL COMMENT '户口所在省（字典city）',
 `HOUSEHOLD_CITY` VARCHAR(16)  DEFAULT NULL  COMMENT '户口所在市（字典city）',
 `ADDRESS_PROVINCE` VARCHAR(16) DEFAULT  NULL COMMENT '常驻地省份（字典city）',
 `ADDRESS_CITY` VARCHAR(16)  DEFAULT NULL COMMENT '常驻地市区（字典city）',
 `MARITAL_STATUS` VARCHAR(2)  DEFAULT NULL COMMENT '婚姻状况（字典：maritalStatus）',
 `HEIGHT` VARCHAR(4)  DEFAULT NULL COMMENT '身高（cm)',
 `WEIGHT` VARCHAR(4)  DEFAULT NULL COMMENT '体重(kg)',
 `EDUCATION` VARCHAR(4)  DEFAULT NULL COMMENT '学历文凭（字典：education）',
 `PHONE_NUMBER` VARCHAR(20) DEFAULT NULL COMMENT '手机号码',
 `CONTACT_INFORMATION` VARCHAR(100) DEFAULT NULL COMMENT '其他联系方式',
 `BIRTHDATE` DATE  NOT NULL COMMENT '出生日期',
 `AGE` VARCHAR(20)   NULL COMMENT '年龄',
 `ZODIAC` VARCHAR(10)   NULL COMMENT '生肖（属相）',
 `CONSTELLATION` VARCHAR(20)   NULL COMMENT '星座',
 `ROOM` VARCHAR(4)   NULL COMMENT '是否有房',
 `VEHICLE` VARCHAR(4)   NULL COMMENT '是否有车',
 `ASSETSEXPLAIN` VARCHAR(2000)   NULL COMMENT '资产说明',
 `REMARKS` VARCHAR(2000)   NULL COMMENT '备注',
 `PHOTO` VARCHAR(64)   NULL COMMENT '照片路径',
 `occupation` varchar(300)  NULL DEFAULT NULL COMMENT '职业',
 `income` varchar(100)  NULL DEFAULT NULL COMMENT '收入',
 `CREATE_USER_ID` VARCHAR(64)  NOT NULL COMMENT '创建人ID',
 `CREATE_ORG_ID` VARCHAR(64)  NOT NULL COMMENT '创建人机构ID',
 `CREATE_DATE` DATE NOT NULL COMMENT '创建日期',
 `UPDATE_USER_ID` VARCHAR(64)   NULL COMMENT '修改人ID',
 `UPDATE_ORG_ID` VARCHAR(64)   NULL COMMENT '修改人机构ID',
 `UPDATE_DATE` DATE  NULL COMMENT '修改日期',
 `IS_DELETE` VARCHAR(2) NOT NULL COMMENT '删除标识（0删除，1未删除）'
) COMMENT = '相亲客户表' ENGINE = INNODB CHARACTER SET = UTF8 COLLATE = UTF8_GENERAL_CI ROW_FORMAT = COMPACT;

#2.匹配表
#DROP TABLE `TB_MATCHMAKING_MATCHING`;
CREATE TABLE `TB_MATCHMAKING_MATCHING` (
 `MATCHING_ID` VARCHAR(64)  NOT NULL PRIMARY KEY COMMENT '主键ID',
 `CUSTOMER_MAIN_ID` VARCHAR(64)  NOT NULL  COMMENT '主要客户id',
 `CUSTOMER_VICE_ID` VARCHAR(64)  NOT NULL COMMENT '被选客户id',
 `CREATE_USER_ID` VARCHAR(64)  NOT NULL COMMENT '创建人ID',
 `CREATE_ORG_ID` VARCHAR(64)  NOT NULL COMMENT '创建人机构ID',
 `CREATE_DATE` DATE NOT NULL COMMENT '创建日期',
 `UPDATE_USER_ID` VARCHAR(64)   NULL COMMENT '修改人ID',
 `UPDATE_ORG_ID` VARCHAR(64)   NULL COMMENT '修改人机构ID',
 `UPDATE_DATE` DATE  NULL COMMENT '修改日期',
 `IS_DELETE` VARCHAR(2) NOT NULL COMMENT '删除标识（0删除，1未删除）'
) COMMENT = '相亲客户表' ENGINE = INNODB CHARACTER SET = UTF8 COLLATE = UTF8_GENERAL_CI ROW_FORMAT = COMPACT;

#2.模型管理表
#DROP TABLE `TB_EXHIBITION3D_MODEL`;
CREATE TABLE `TB_EXHIBITION3D_MODEL` (
 `MODEL_ID` VARCHAR(64)  NOT NULL PRIMARY KEY COMMENT '主键ID',
 `MODEL_NAME` VARCHAR(200)  NOT NULL  COMMENT '模型数据名称',
 `MODEL_TYPE` VARCHAR(64)  DEFAULT NULL  COMMENT '模型类型（字典modelType)',
 `MODEL_FILE` VARCHAR(64)   NULL COMMENT '模型文件id',
 `REMARKS` VARCHAR(2000)   NULL COMMENT '备注信息',
 `CREATE_USER_ID` VARCHAR(64)  NOT NULL COMMENT '创建人ID',
 `CREATE_ORG_ID` VARCHAR(64)  NOT NULL COMMENT '创建人机构ID',
 `CREATE_DATE` DATE NOT NULL COMMENT '创建日期',
 `UPDATE_USER_ID` VARCHAR(64)   NULL COMMENT '修改人ID',
 `UPDATE_ORG_ID` VARCHAR(64)   NULL COMMENT '修改人机构ID',
 `UPDATE_DATE` DATE  NULL COMMENT '修改日期',
 `IS_DELETE` VARCHAR(2) NOT NULL COMMENT '删除标识（0删除，1未删除）'
) COMMENT = '相亲客户表' ENGINE = INNODB CHARACTER SET = UTF8 COLLATE = UTF8_GENERAL_CI ROW_FORMAT = COMPACT;

